{
  "name": "chrono",
  "private": true,
  "scripts": {
    "test": "standard",
    "cover": "nyc --reporter=html npm test",
    "build": "copy-files 'base/*' build && assets ./ build && framework render renderer.js && optimize build build/bundle.css --inline",
    "watch": "copy-files 'base/*' build -w & assets ./ build -w --no-min & serve-files build --default 200"
  },
  "devDependencies": {
    "faucet": "0.0.1",
    "nyc": "^11.0.3",
    "standard": "^10.0.0",
    "tape": "^4.7.0"
  },
  "dependencies": {
    "@erickmerchant/assets": "^6.1.2",
    "@erickmerchant/copy-files": "^1.2.2",
    "@erickmerchant/framework": "^11.0.1",
    "@erickmerchant/framework-cli": "^1.0.4",
    "@erickmerchant/optimize": "^3.1.0",
    "@erickmerchant/serve-files": "^2.0.4",
    "animate.css": "^3.2.5",
    "geomicons-open": "^2.0.0",
    "yo-yo": "^1.4.1"
  }
}
