{
  "name": "chrono",
  "private": true,
  "scripts": {
    "test": "standard",
    "cover": "nyc --reporter=html npm test",
    "build": "copy-files base/* build && assets . build && framework render fixtures/*.json js/component.js build/200.html && optimize build build/bundle.css --inline",
    "watch": "copy-files base/* build -w & assets . build -w --no-min & serve-files build --default 200"
  },
  "devDependencies": {
    "faucet": "0.0.1",
    "nyc": "^11.2.1",
    "standard": "^10.0.3",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "@erickmerchant/assets": "^6.4.7",
    "@erickmerchant/copy-files": "^1.2.8",
    "@erickmerchant/framework": "^12.1.2",
    "@erickmerchant/framework-cli": "^3.0.6",
    "@erickmerchant/optimize": "^5.0.8",
    "@erickmerchant/serve-files": "^2.1.4",
    "animate.css": "^3.2.5",
    "bel": "^5.1.1",
    "geomicons-open": "^2.0.0",
    "nanomorph": "^5.1.2"
  }
}
