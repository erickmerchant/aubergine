{
  "name": "chrono",
  "private": true,
  "scripts": {
    "test": "standard && tape 'test/**/*.js' | faucet",
    "cover": "nyc --reporter=html npm test",
    "build": "npm run test && copy-files 'base/*' build && assets ./ build && framework render renderer.js && optimize build build/bundle.css --inline",
    "watch": "copy-files 'base/*' build -w & assets ./ build -w --no-min & serve-files build --default 200"
  },
  "devDependencies": {
    "assert-snapshot": "^1.0.1",
    "faucet": "0.0.1",
    "nyc": "^11.0.3",
    "standard": "^10.0.0",
    "tape": "^4.7.0"
  },
  "dependencies": {
    "@erickmerchant/assets": "^6.1.0",
    "@erickmerchant/copy-files": "^1.2.1",
    "@erickmerchant/framework": "^11.0.1",
    "@erickmerchant/framework-cli": "^1.0.3",
    "@erickmerchant/optimize": "^3.1.0",
    "@erickmerchant/serve-files": "^2.0.3",
    "animate.css": "^3.2.5",
    "bel": "^5.0.2",
    "geomicons-open": "^2.0.0",
    "nanomorph": "^5.1.2"
  }
}
